import java.util.*;

 // Compiler version JDK 11.0.2

 class SumTerm
 {
   public static void main(String args[])
   { 
     Scanner sc = new Scanner(System.in);
     
     System.out.println("Enter No. ");
     double n=sc.nextDouble();
     double sum=0;
     for(int i=1; i<=n ;i++)
     {
       sum=sum+1.0/i;
     }
     System.out.println("sum of no is"+sum);
   }
 }
